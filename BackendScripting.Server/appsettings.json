{
  // assembly cache timeout, use a zero timespan to disable the assembly cache (default: 30 minutes)
  "AssemblyCacheTimeout": "00:30:00",

  /// script function timeout (default: 10 seconds)
  "ScriptFunctionTimeout": "00:00:10",

  // initialize the script compiler to reduce first execution time (default: false)
  "InitializeScriptCompiler": true,

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DbConnectionString": "server=localhost; database=BusinessApi; Integrated Security=SSPI; TrustServerCertificate=True"
  }
}
